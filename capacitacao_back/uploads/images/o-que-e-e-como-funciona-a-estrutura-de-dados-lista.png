    <script>
        document.getElementById('blog_post_newsletter').addEventListener('submit', function (event) {
            event.preventDefault();

            const form = event.target;
            const formData = new FormData(form);

            const xhr = new XMLHttpRequest();
            xhr.open('POST', form.action, true);

            xhr.onload = function () {
                if (xhr.status === 200) {
                    alert('Seu e-mail foi cadastrado com sucesso!');
                    form.reset();
                } else {
                    alert('Erro: Por gentileza, tente novamente.');
                }
            };

            xhr.send(formData);
        });

        document.addEventListener('DOMContentLoaded', function () {
            let exitPopupOpened = false;
            document.addEventListener("mouseleave", (event) => {
                if (event.clientY <= 0) {
                    if (!exitPopupOpened) {
                        document.querySelector('#modal-exit-popup').showModal();
                        exitPopupOpened = true;
                    }
                }
            });

            const dialog = document.querySelector("dialog");
            dialog.addEventListener("click", function(event) {
                if (event.target === dialog) {
                    dialog.close();
                }
            });
        });
    </script>
